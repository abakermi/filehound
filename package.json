{
  "name": "filehound",
  "version": "1.16.0",
  "description": "Find files the easy way",
  "main": "index.js",
  "scripts": {
    "jsdoc": "jsdoc -c ./docconfig/jsdoc.json",
    "lint": "eslint ./src ./test",
    "coverage": "node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha -- -R dot",
    "compile": "babel --presets es2015,stage-3 -d lib/ src/",
    "clean": "rm lib/*",
    "prepublish": "npm run compile",
    "test": "npm run compile && mocha --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nspragg/filehound.git"
  },
  "keywords": [
    "file",
    "recursive",
    "search",
    "filehound",
    "file",
    "hound",
    "lister",
    "find",
    "directory",
    "walk",
    "walker",
    "promise",
    "finder",
    "fs"
  ],
  "author": "nspragg@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nspragg/filehound/issues"
  },
  "homepage": "https://github.com/nspragg/filehound",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.1.1",
    "babel-istanbul": "^0.12.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-3": "^6.22.0",
    "coveralls": "^2.11.9",
    "eslint": "^3.17.1",
    "istanbul": "^0.4.4",
    "jsdoc": "^3.4.3",
    "mocha": "^3.0.0",
    "sinon": "^1.17.4"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "file-js": "0.3.0",
    "lodash": "^4.13.1",
    "minimatch": "^3.0.2",
    "moment": "^2.14.1",
    "unit-compare": "^1.0.1"
  }
}
